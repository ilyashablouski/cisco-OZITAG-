<div class="dashboard">
  <span class="dashboard__title">My Passport</span>

  <div class="dashboard__cards">
    <div class="dashboard__card">
      {{> widgets/card
              flag-icon="flags/flag_india"
              title="India"
              completed=true
              bottom-label="Video materials"
              label-icon='video'
      }}
    </div>

    <div class="dashboard__card">
      {{> widgets/card
              btn-top=true
              title="Germany"
              incompleted=true
              bottom-label="Text materials"
              label-icon='text'
      }}
    </div>

    <div class="dashboard__card">
      {{> widgets/card
              btn-top=true
              title="Italy"
              inprogress=true
              bottom-label="Presentation materials"
              label-icon='presentation'
      }}
    </div>

    <div class="dashboard__card">
      {{> widgets/card
              flag-icon="flags/flag_australia"
              title="Australia"
              completed=true
              bottom-label="Video materials"
              label-icon='video'
      }}
    </div>

    <div class="dashboard__card">
      {{> widgets/card
              btn-top=true
              title="Indonesia"
              incompleted=true
              bottom-label="Text materials"
              label-icon='text'
      }}
    </div>

    <div class="dashboard__card">
      {{> widgets/card
              btn-top=true
              title="Argentina"
              incompleted=true
              bottom-label="Text materials"
              label-icon='text'
      }}
    </div>
  </div>


  <div class="dashboard__container">
    <div class="dashboard__block">
      <div class="dashboard__block-item">
        {{> blocks/page-dashboard/progress-card}}
      </div>

      <div class="dashboard__block-item">
        <div class="events-slider">
          {{> blocks/page-dashboard/event-slide}}
        </div>
      </div>
    </div>

    <div class="dashboard__block">

    </div>
  </div>
</div>
